Algoritmo Ejercicio16
	
	//Calcular el área de un rectángulo de lados X e Y.
	
	//DECLARACION VARIABLES
	Definir LargoRectangulo Como Real;
	Definir AnchoRectangulo Como Real;
	Definir AreaRectangulo Como Real;
	
	//ASIGNACION VARIABLES
	LargoRectangulo <- 6;
	AnchoRectangulo <- 2;
	
	//PROCESOS - OPERACIONES - FORMULAS
	AreaRectangulo <- LargoRectangulo * AnchoRectangulo;
	
	//SALIDA DATOS
	Escribir "El Area del Rectangulo es: ", AreaRectangulo, " cm";
	
FinAlgoritmo
